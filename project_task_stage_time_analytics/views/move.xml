<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="project_task_analytics_status_time_view_tree" model="ir.ui.view">
        <field name="model">project.task.analytics.move</field>
        <field name="arch" type="xml">
            <tree create="false">
                <field name="id"/>
                <field name="project_task_id"/>
                <field name="previous_stage_id"/>
                <field name="after_stage_id"/>
                <field name="create_date" string="Initial Date"/>
                <field name="end_date" string="End Date"/>
                <field name="time_days" string="Time (days)" sum="Total Time"/>
                <field name="avg_stage_time" avg="Avg time/stage"/>
                <field name="user_id_ref"/>
            </tree>
        </field>
    </record>
    <record id="project_task_analytics_move_action" model="ir.actions.act_window">
        <field name="name">Project Task Analytics</field>
        <field name="res_model">project.task.analytics.move</field>
        <field name="view_mode">tree,pivot</field>
    </record>
    <menuitem id="project_task_analytics" name="Project Task Analytics" parent="project.menu_project_report" action="project_task_analytics_move_action" sequence="9"/>
</odoo>
