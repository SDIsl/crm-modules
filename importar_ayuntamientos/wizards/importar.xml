<odoo>

    <record id="importar_ayuntamientos_form" model="ir.ui.view">
        <field name="name">importar.contratos.wizard.form</field>
        <field name="model">importar.ayuntamientos</field>
        <field name="mode">primary</field>
        <field name="arch" type="xml">
            <form>
                <group name="main_info">
                    <field name="data_file"/>
                </group>
                <footer>
                    <button name="button_importar" string="Importar"
                            type="object" default_focus="1" class="oe_highlight"/>
                    or
                    <button string="Cancel" class="oe_link" special="cancel" />
                </footer>
            </form>
        </field>
    </record>

    <act_window
            id="action_importar_ayuntamientos_wizard"
            name="Importar ayuntamientos"
            res_model="importar.ayuntamientos"
            src_model="importar.ayuntamientos"
            view_mode="form"
            target="new"
            key2="client_action_multi"
    />

    <menuitem
            id="action_importar_contratos_menu"
            name="Importar contratos"
            parent="contacts.menu_contacts"
            sequence="990"
            action="action_importar_ayuntamientos_wizard"
            groups="base.group_no_one"/>


</odoo>
