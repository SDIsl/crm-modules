<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="res_users_view_form" model="ir.ui.view">
        <field name="model">res.users</field>
        <field name="inherit_id" ref="openapi.res_users_view_form"/>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='openapi_token']" position="after">
                <field name="is_openapi_mail_receiver"/>
            </xpath>
        </field>
    </record>
</odoo>
