        <field name="name">External Links</field>
        <field name="res_model">external.link</field>
        <field name="view_mode">tree</field>
    </record>
    <record id="action_list_external_link_groups" model="ir.actions.act_window">
        <field name="name">External Link Groups</field>
        <field name="res_model">external.link.group</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem id="menu_external_links_root" name="External Links" web_icon="external_links,static/description/icon.png"/>
    <menuitem id="menu_external_links" name="External Links" parent="menu_external_links_root" action="action_list_external_links" sequence="1"/>
    <menuitem id="menu_external_link_groups" name="External Link Groups" parent="menu_external_links_root" action="action_list_external_link_groups" sequence="2"/>
